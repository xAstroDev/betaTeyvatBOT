{
  "format": 2,
  "name": {
    "_code": "3793925636",
    "zh": "丘丘萨满",
    "de": "Samachurl",
    "en": "Samachurl",
    "es": "Samachurl",
    "fr": "Brutoshaman",
    "id": "Samachurl",
    "ja": "ヒルチャールシャーマン",
    "ko": "츄츄 샤먼",
    "pt": "Samachurl",
